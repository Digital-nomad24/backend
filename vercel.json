{
    "version":2,
    "builds": [
        {
            "src":"index.js",
            "use":"@vercel/node"
        }
    ],
    "rewrites":[
            {"source": "/(.*)", "destination": "/"}
     ],
     "routes": [
        {
            "src": "/api/v1/(.*)",
            "headers": {
              "Access-Control-Allow-Origin": "*"
            },
            "dest": "/api/v1/$1"
          }
        ]
      }