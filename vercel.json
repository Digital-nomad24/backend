{
    "version":2,
    "builds": [
        {
            "src":"index.js",
            "use":"@vercel/node"
        }
    ],
    "rewrites":[
            {"source": "/(.*)", "destination": "/"}
     ],
     "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/$1"
    }
  ]
      }